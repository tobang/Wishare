{
  "projectId": "694ab35e00384ed4a1a5",
  "projectName": "Wishare",
  "buckets": [
    {
      "$id": "wish-images",
      "name": "Wish Images",
      "enabled": true,
      "maximumFileSize": 5242880,
      "allowedFileExtensions": ["jpg", "jpeg", "png", "gif", "webp"],
      "compression": "gzip",
      "encryption": true,
      "antivirus": true,
      "fileSecurity": true,
      "$permissions": ["create(\"users\")", "read(\"users\")"]
    }
  ],
  "functions": [
    {
      "$id": "scrapeMetaTags",
      "name": "Scrape Meta Tags",
      "runtime": "node-21.0",
      "path": "functions/scrapeMetaTags",
      "commands": "npm install",
      "entrypoint": "src/main.js",
      "ignore": ["node_modules", ".npm", "*.md", ".git", ".gitignore"],
      "execute": ["any"],
      "events": [],
      "schedule": "",
      "timeout": 30,
      "enabled": true,
      "logging": true,
      "variables": {}
    }
  ],
  "databases": [
    {
      "$id": "wishare",
      "name": "Wishare Database",
      "enabled": true
    }
  ],
  "collections": [
    {
      "$id": "wishes",
      "$permissions": [
        "create(\"users\")",
        "read(\"users\")",
        "update(\"users\")",
        "delete(\"users\")"
      ],
      "databaseId": "wishare",
      "name": "Wishes",
      "enabled": true,
      "documentSecurity": true,
      "attributes": [
        {
          "key": "price",
          "type": "double",
          "required": false,
          "array": false,
          "min": 0,
          "max": 999999.99,
          "default": null
        },
        {
          "key": "priority",
          "type": "integer",
          "required": false,
          "array": false,
          "min": 1,
          "max": 999,
          "default": 1
        },
        {
          "key": "quantity",
          "type": "integer",
          "required": false,
          "array": false,
          "min": 1,
          "max": 999,
          "default": 1
        },
        {
          "key": "title",
          "type": "string",
          "required": true,
          "array": false,
          "size": 255,
          "default": null,
          "encrypt": false
        },
        {
          "key": "description",
          "type": "string",
          "required": false,
          "array": false,
          "size": 2000,
          "default": null,
          "encrypt": false
        },
        {
          "key": "uid",
          "type": "string",
          "required": true,
          "array": false,
          "size": 255,
          "default": null,
          "encrypt": false
        },
        {
          "key": "url",
          "type": "string",
          "required": false,
          "array": false,
          "size": 1000,
          "default": null,
          "encrypt": false
        },
        {
          "key": "image",
          "type": "string",
          "required": false,
          "array": false,
          "size": 5000,
          "default": null,
          "encrypt": false
        },
        {
          "key": "files",
          "type": "string",
          "required": false,
          "array": true,
          "size": 255,
          "default": null,
          "encrypt": false
        },
        {
          "key": "wlid",
          "type": "string",
          "required": true,
          "array": false,
          "size": 255,
          "default": null,
          "encrypt": false
        }
      ],
      "indexes": [
        {
          "key": "uid_idx",
          "type": "key",
          "status": "available",
          "attributes": ["uid"],
          "orders": ["ASC"]
        },
        {
          "key": "wlid_idx",
          "type": "key",
          "status": "available",
          "attributes": ["wlid"],
          "orders": ["ASC"]
        }
      ]
    },
    {
      "$id": "wishlists",
      "$permissions": [
        "create(\"users\")",
        "read(\"users\")",
        "update(\"users\")",
        "delete(\"users\")"
      ],
      "databaseId": "wishare",
      "name": "Wishlists",
      "enabled": true,
      "documentSecurity": true,
      "attributes": [
        {
          "key": "uid",
          "type": "string",
          "required": true,
          "array": false,
          "size": 255,
          "default": null,
          "encrypt": false
        },
        {
          "key": "priority",
          "type": "integer",
          "required": false,
          "array": false,
          "min": 1,
          "max": 999,
          "default": 1
        },
        {
          "key": "visibility",
          "type": "string",
          "required": false,
          "array": false,
          "elements": ["draft", "published", "archived"],
          "format": "enum",
          "default": "draft"
        },
        {
          "key": "title",
          "type": "string",
          "required": true,
          "array": false,
          "size": 255,
          "default": null,
          "encrypt": false
        },
        {
          "key": "description",
          "type": "string",
          "required": false,
          "array": false,
          "size": 1000,
          "default": null,
          "encrypt": false
        }
      ],
      "indexes": [
        {
          "key": "uid_idx",
          "type": "key",
          "status": "available",
          "attributes": ["uid"],
          "orders": ["ASC"]
        },
        {
          "key": "priority_uid_idx",
          "type": "unique",
          "status": "available",
          "attributes": ["priority", "uid"],
          "orders": ["ASC", "ASC"]
        }
      ]
    }
  ]
}
